{
  "name": "rd",
  "author": "github.com/jannden",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "dev": "dotenv -e .env.local -- next dev",
    "build": "prisma generate && prisma migrate deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:studio": "dotenv -e .env.local -- prisma studio",
    "db:generate": "prisma format && dotenv -e .env.local -- prisma generate",
    "db:migrate": "prisma format && dotenv -e .env.local -- prisma migrate dev && dotenv -e .env.local -- prisma db seed",
    "db:push": "prisma format && dotenv -e .env.local -- prisma db push && dotenv -e .env.local -- prisma db seed",
    "db:seed": "dotenv -e .env.local -- prisma db seed"
  },
  "dependencies": {
    "@clerk/localizations": "^1.26.16",
    "@clerk/nextjs": "latest",
    "@headlessui/react": "^1.7.18",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^5.10.2",
    "@t3-oss/env-nextjs": "^0.9.2",
    "@types/remark-prism": "^1.3.7",
    "@vercel/analytics": "^1.2.2",
    "ai": "^3.0.8",
    "best-effort-json-parser": "^1.0.1",
    "boring-avatars": "^1.10.1",
    "encoding": "^0.1.13",
    "eslint-config-next": "^14.1.3",
    "gpt-tokenizer": "^2.1.2",
    "logrocket": "^8.1.0",
    "logrocket-react": "^6.0.3",
    "lottie-react": "^2.4.0",
    "lucide-react": "^0.356.0",
    "luxon": "^3.4.4",
    "next": "^14.1.3",
    "openai": "^4.28.4",
    "postmark": "^4.0.2",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-simple-keyboard": "^3.7.94",
    "react-syntax-highlighter": "^15.5.0",
    "react-textarea-autosize": "^8.5.3",
    "react-toastify": "^10.0.4",
    "react-tooltip": "^5.26.3",
    "recharts": "^2.12.2",
    "rehype-highlight": "^7.0.0",
    "remark-prism": "^1.3.6",
    "sharp": "^0.33.2",
    "simple-keyboard-layouts": "^3.3.83",
    "stripe": "^14.20.0",
    "svix": "^1.21.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/logrocket-react": "^3.0.3",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.25",
    "@types/prismjs": "^1.26.3",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@typescript-eslint/parser": "^7.1.1",
    "autoprefixer": "^10.4.18",
    "clsx": "^2.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-tailwindcss": "^3.15.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prisma": "^5.10.2",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
