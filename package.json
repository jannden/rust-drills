{
  "name": "rust-drills",
  "author": "github.com/jannden",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "dev": "dotenv -e .env.local -- next dev",
    "build": "prisma generate && prisma migrate deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:studio": "dotenv -e .env.local -- prisma studio",
    "db:generate": "prisma format && dotenv -e .env.local -- prisma generate",
    "db:migrate": "prisma format && dotenv -e .env.local -- prisma migrate dev && dotenv -e .env.local -- prisma db seed",
    "db:push": "prisma format && dotenv -e .env.local -- prisma db push && dotenv -e .env.local -- prisma db seed",
    "db:seed": "dotenv -e .env.local -- prisma db seed"
  },
  "dependencies": {
    "@clerk/nextjs": "latest",
    "@headlessui/react": "^1.7.19",
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@next/mdx": "^14.2.2",
    "@prisma/client": "^5.12.1",
    "@t3-oss/env-nextjs": "^0.9.2",
    "@types/logrocket-react": "^3.0.3",
    "@types/mdx": "^2.0.13",
    "@types/remark-prism": "^1.3.7",
    "@uiw/react-textarea-code-editor": "^3.0.2",
    "@vercel/analytics": "^1.2.2",
    "ai": "^3.0.24",
    "encoding": "^0.1.13",
    "eslint-config-next": "^14.2.2",
    "gpt-tokenizer": "^2.1.2",
    "logrocket": "^8.1.0",
    "logrocket-react": "^6.0.3",
    "lucide-react": "^0.372.0",
    "luxon": "^3.4.4",
    "next": "^14.2.2",
    "openai": "^4.38.2",
    "postmark": "^4.0.2",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "react-textarea-autosize": "^8.5.3",
    "react-toastify": "^10.0.5",
    "react-tooltip": "^5.26.3",
    "recharts": "^2.12.5",
    "rehype-highlight": "^7.0.0",
    "remark-prism": "^1.3.6",
    "sharp": "^0.33.3",
    "simple-keyboard-layouts": "^3.3.98",
    "stripe": "^15.3.0",
    "svix": "^1.21.0",
    "zod": "^3.22.5"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.12",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.12.7",
    "@types/prismjs": "^1.26.3",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@typescript-eslint/parser": "^7.7.0",
    "autoprefixer": "^10.4.19",
    "clsx": "^2.1.0",
    "eslint": "^9.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prisma": "^5.12.1",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5"
  }
}
